-- Table quiz2
CREATE TABLE quiz2(
    Q_ID INT PRIMARY KEY,
    Questions varchar(200),
    A VARCHAR(100),
    B VARCHAR(100),
    C VARCHAR(100),
    D VARCHAR(100),
    ANSWER VARCHAR(5)
);

INSERT INTO quiz2 VALUES(
