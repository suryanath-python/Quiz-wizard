-- Table quiz1
CREATE TABLE quiz1(
    Q_ID INT PRIMARY KEY,
    Questions varchar(200),
    A VARCHAR(100),
    B VARCHAR(100),
    C VARCHAR(100),
    D VARCHAR(100),
    ANSWER VARCHAR(5)
);

INSERT INTO quiz1 VALUES(
    1,
    'What is the SI unit of electric current?',
    'A) Volt',
    'B) Ampere',
    'C) Ohm',
    'D) Coulomb',
    'B'
);

INSERT INTO quiz1 VALUES(
    2,
    'Which law of thermodynamics states that energy cannot be created or destroyed, only transformed?',
    'A) Zeroth Law',
    'B) First Law',
    'C) Third Law',
    'D) Fourth Law',
    'B'
);

INSERT INTO quiz1 VALUES(
    3,
    'Unit of capacitance??',
    'A) Farad',
    'B) Ampere',
    'C) Ohm',
    'D) Coulomb',
    'A'
);

